<h1>Books</h1>

<% @book.each do |book| %>
  <p>brank</p>
  <span>
    <%= link_to book.title, book_path(book.id) %>
  </span>
<% end %>


<%= form_with model: Book.new do |f| %>
  <h4>New book</h4>
  <p>Title</p>
  <%= f.text_field :title %>

  <p>Body</p>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>

<% end %>